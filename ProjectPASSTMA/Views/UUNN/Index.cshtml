@model IEnumerable<ENTIDAD.UUNNCE>

@{
    ViewBag.Title = "SE/GERENCIA";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>SE/GERENCIA</h2>
<p>
    <a class="btn btn-success" href="@Url.Action("Crear", "UUNN")"><i class="glyphicon glyphicon-plus"></i>Crear Nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                ID
            </th>
            <th>
                NOMBRE
            </th>
            <th>
                UN/SEM
            </th>
            <th></th>
        </tr>
    </thead>


    @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.IdUUNN
                </td>
                <td>
                    @item.NombreUUNN
                </td>
                <td>
                    @item.NombreSEDE
                </td>
                <td>
                    <a class="btn btn-primary btn-xs" href="@Url.Action("Editar", "UUNN", new { id = item.IdUUNN })" title="Editar"><i class="glyphicon glyphicon-pencil"></i></a>
                    <a class="btn btn-info btn-xs" href="@Url.Action("Detalle", "UUNN", new { id = item.IdUUNN })" title="Detalles"><i class="glyphicon glyphicon-th-list"></i></a>
                    <a class="btn btn-danger btn-xs" href="@Url.Action("Eliminar", "UUNN", new { id = item.IdUUNN })" title="Eliminar"><i class="glyphicon glyphicon-trash"></i></a>
                </td>
            </tr>
        }

</table>
